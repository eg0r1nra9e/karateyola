// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../database/dev.db"
}

model City {
  id   Int     @id @default(autoincrement())
  region String
  city String? @unique
}

model Team {
  id   Int     @id @default(autoincrement())
  name String  @unique
  city String?
}
